  <body>
    {{>navbar}}
    <div class="skill_container">
      <h1 id="Skill_tagline">Skills at a Glance</h1>
      <div class="row row-cols-1 row-cols-md-2 g-4">
        <div class="col">
          <div class="card">
            <!-- <img src="" class="card-img-top" alt="..." /> -->
            <div class="card-body">
              <h5 id="h5_card" class="card-title">FrontEnd</h5>
              <p class="card-text">
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="button"
                  value="ReactJS"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="submit"
                  value="HTML"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="reset"
                  value="Wordpress"
                />
              </p>
            </div>
          </div>
        </div>

        <div class="col">
          <div class="card">
            <!-- <img src="" class="card-img-top" alt="..." /> -->
