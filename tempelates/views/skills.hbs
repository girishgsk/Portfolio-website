              <h5 id="h5_card" class="card-title">BackEnd</h5>
              <p class="card-text">
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="button"
                  value="NodeJS"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="submit"
                  value="ExpressJS"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="submit"
                  value="HBS"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="reset"
                  value="PHP"
                />
              </p>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card">
            <!-- <img src="..." class="card-img-top" alt="..." /> -->
            <div class="card-body">
              <h5 id="h5_card" class="card-title">Styling</h5>
              <p class="card-text">
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="button"
                  value="CSS"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="submit"
                  value="Bootstrap"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="reset"
                  value="FlexBox"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="reset"
                  value="SCSS"
                />
              </p>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card">
            <!-- <img src="" class="card-img-top" alt="..." /> -->
            <div class="card-body">
              <h5 id="h5_card" class="card-title">Database</h5>
              <p class="card-text">
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="button"
                  value="MongoDB"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="submit"
                  value="SQL"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="reset"
                  value="MySQL"
                />
              </p>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card">
            <!-- <img src="..." class="card-img-top" alt="..." /> -->
            <div class="card-body">
              <h5 id="h5_card" class="card-title">Programming Languages</h5>
              <p class="card-text">
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="button"
                  value="Javascript"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="submit"
                  value="Python"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="reset"
                  value="C++"
                />
                <input
                  class="btn btn-primary"
                  id="skill_btn"
                  type="reset"
                  value="C"
                />
              </p>
            </div>
          </div>
